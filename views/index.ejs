<!DOCTYPE html>
<html lang="sv">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Kurslista</title>
    <link rel="stylesheet" href="/css/styles.css">
</head>
<body>

    <div class="container">
        <main class="main-container">
            <a href="/">Startsida</a>
            <a href="/addcourse">LÃ¤gg till kurs</a>
            <a href="/about">Om sidan</a>

            <h1>Kurslista</h1>
            <% if(rows.length > 0) { %>
                <% rows.forEach((course) => { %>
                    <article>
                        <p><%= course.coursecode %></p>
                        <p><%= course.coursename %></p>
                        <p><%= course.progression %></p>
                        <p><%= course.syllabus %></p>
                    </article>
                <% }); %>
           <% } %>
        </main>
    </div>
</body>
</html>